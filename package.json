    "build": "lerna exec --scope onekijs* -- npm run build",
    "build:fast": "lerna exec --scope onekijs* -- yarn build:fast",
    "ts": "lerna exec --scope onekijs -- yarn start",
    "cm": "git-cz",
    "lerna-version": "lerna version --conventional-commits",
