{
  "name": "onekijs-core",
  "version": "0.7.1",
  "description": "Opiniated React framework built on top of Redux and React Router",
  "author": "oneki",
  "license": "MIT",
  "repository": "oneki/onekijs",
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "build": "babel --root-mode upward src -d dist/esm --source-maps",
    "start": "babel --root-mode upward src -d dist/esm --watch --source-maps",
    "prepare": "npm run build",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build"
  },
  "peerDependencies": {
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0"
  },
  "dependencies": {
    "immer": "^6.0.2",
    "query-string": "^6.11.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3",
    "rfc4648": "^1.3.0",
    "styled-components": "^5.0.1",
    "text-encoding-shim": "^1.0.5"
  },
  "sideEffects": false,
  "gitHead": "98efc50b0562574c7a0318edbc1eaa650e8dc26f"
}
