{
  "name": "onekijs-framework",
  "author": "Oneki",
  "version": "0.15.8",
  "description": "Opiniated React framework built on top of Redux and React Router",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "engines": {
    "node": ">=10"
  },
  "exports": {
    ".": {
      "import": "./dist/index.modern.js",
      "require": "./dist/index.js"
    }
  },
  "module": "dist/index.modern.js",
  "unpkg": "dist/index.umd.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "rimraf dist && rollup --config rollup.config.js",
    "start": "rollup -w --config rollup.config.js"
  },
  "license": "MIT",
  "dependencies": {
    "@types/jest": "^28.1.6",
    "@types/node": "^18.7.1",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "history": "^5.3.0",
    "immer": "^9.0.15",
    "query-string": "^7.1.1",
    "react-redux": "^8.0.2",
    "react-transition-group": "^4.4.2",
    "redux": "^4.2.0",
    "redux-saga": "^1.1.3",
    "reflect-metadata": "^0.1.13",
    "rfc4648": "^1.5.2",
    "text-encoding-shim": "^1.0.5"
  },
  "devDependencies": {
    "@types/react-transition-group": "^4.4.0",
    "concurrently": "^7.3.0",
    "microbundle": "^0.15.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.77.3"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "sideEffects": false
}
