{
  "name": "onekijs-next-ssr-old",
  "private": true,
  "version": "0.11.0",
  "description": "Opiniated React framework built on top of Next.js",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "exports": {
    ".": {
      "import": "./dist/index.modern.js",
      "require": "./dist/index.js"
    }
  },
  "module": "dist/index.module.js",
  "unpkg": "dist/index.umd.js",
  "scripts": {
    "build": "microbundle --jsx React.createElement --no-compress",
    "start": "microbundle watch --jsx React.createElement --no-compress"
  },
  "author": "Oneki",
  "license": "MIT",
  "dependencies": {
    "@oneki/app": "^0.11.0",
    "@oneki/auth": "^0.11.0",
    "@oneki/collection": "^0.11.0",
    "@oneki/core": "^0.11.0",
    "@oneki/fetch": "^0.11.0",
    "@oneki/form": "^0.11.0",
    "@oneki/i18n": "^0.11.0",
    "@oneki/notification": "^0.11.0",
    "@oneki/router": "^0.11.0",
    "@oneki/types": "^0.11.0",
    "@oneki/utils": "^0.11.0",
    "history": "^4.10.1",
    "immer": "^8.0.1",
    "next": "^11.0.1",
    "query-string": "^6.11.1",
    "react-redux": "^7.2.0",
    "react-transition-group": "^4.4.1",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3",
    "reflect-metadata": "^0.1.13",
    "rfc4648": "^1.3.0",
    "regenerator-runtime": "0.13.9",
    "text-encoding-shim": "^1.0.5"
  },
  "devDependencies": {
    "microbundle": "^0.13.3"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16"
  },
  "sideEffects": false
}
