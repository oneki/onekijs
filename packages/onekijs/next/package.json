{
  "name": "onekijs-next",
  "private": true,
  "version": "0.11.0",
  "description": "Opiniated React framework built on top of Next.js",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "exports": {
    ".": {
      "import": "./dist/index.modern.js",
      "require": "./dist/index.js"
    }
  },
  "module": "dist/index.module.js",
  "unpkg": "dist/index.umd.js",
  "scripts": {
    "build": "microbundle --jsx React.createElement --no-compress",
    "build:ssr": "microbundle build --cwd ssr",
    "start": "microbundle watch --jsx React.createElement --no-compress"
  },
  "author": "Oneki",
  "license": "MIT",
  "dependencies": {
    "next": "^11.0.1",
    "onekijs": "^0.11.0"
  },
  "devDependencies": {
    "microbundle": "^0.13.3"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16"
  },
  "sideEffects": false
}
