{
  "name": "onekijs",
  "version": "0.11.0",
  "description": "Opiniated React framework built on top of Redux and React Router",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "exports": {
    ".": {
      "import": "./dist/index.modern.js",
      "require": "./dist/index.js"
    },
    "./next": {
      "import": "./next/dist/index.modern.js",
      "require": "./next/dist/index.js"
    },
    "./next-ssr": {
      "import": "./next-ssr/dist/index.modern.js",
      "require": "./next-ssr/dist/index.js"
    }
  },
  "module": "dist/index.module.js",
  "unpkg": "dist/index.umd.js",
  "scripts": {
    "build": "concurrently \"yarn build:onekijs\" \"yarn build:cra\" \"yarn build:next\" \"yarn build:next-ssr\" ",
    "build:onekijs": "microbundle -f es,cjs,modern --jsx React.createElement",
    "build:cra": "microbundle -f es,cjs,modern --jsx React.createElement --prefix cra",
    "build:next": "microbundle -f es,cjs,modern --jsx React.createElement --prefix next",
    "build:next-ssr": "microbundle -f es,cjs,modern --jsx React.createElement --prefix next-ssr",
    "start": "microbundle watch -f es,cjs,modern --jsx React.createElement"
  },
  "author": "Oneki",
  "license": "MIT",
  "dependencies": {
    "@oneki/app": "^0.11.0",
    "@oneki/auth": "^0.11.0",
    "@oneki/collection": "^0.11.0",
    "@oneki/core": "^0.11.0",
    "@oneki/fetch": "^0.11.0",
    "@oneki/form": "^0.11.0",
    "@oneki/i18n": "^0.11.0",
    "@oneki/notification": "^0.11.0",
    "@oneki/router": "^0.11.0",
    "@oneki/types": "^0.11.0",
    "@oneki/utils": "^0.11.0",
    "history": "^4.10.1",
    "immer": "^8.0.1",
    "query-string": "^6.11.1",
    "react-redux": "^7.2.0",
    "react-transition-group": "^4.4.1",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3",
    "reflect-metadata": "^0.1.13",
    "rfc4648": "^1.3.0",
    "text-encoding-shim": "^1.0.5"
  },
  "devDependencies": {
    "microbundle": "^0.13.3",
    "concurrently": "^6.2.0"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16"
  },
  "sideEffects": false,
  "gitHead": "bd34719c6e0a4119cec72b64ce8fe6b35d1c342f"
}
