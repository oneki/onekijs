{
  "name": "onekijs",
  "version": "0.14.7",
  "description": "Opiniated React framework built on top of Next.js",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "exports": {
    ".": {
      "import": "./dist/index.modern.js",
      "require": "./dist/index.js"
    }
  },
  "module": "dist/index.modern.js",
  "unpkg": "dist/index.umd.js",
  "scripts": {
    "build": "microbundle -f modern --jsx React.createElement --jsxFragment React.Fragment",
    "build:fast": "microbundle -f modern --jsx React.createElement --jsxFragment React.Fragment --no-compress",
    "start": "microbundle watch -f modern --jsx React.createElement --jsxFragment React.Fragment --max-old-space-size=8192 --no-compress",
    "rollup:clean": "rimraf dist",
    "rollup:build": "npm run rollup:clean && rollup --config rollup.config.js",
    "rollup:start": "rollup -w --config rollup.config.js"
  },
  "author": "Oneki",
  "license": "MIT",
  "dependencies": {
    "history": "^4.10.1",
    "onekijs-framework": "^0.14.7",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0"
  },
  "devDependencies": {
    "microbundle": "^0.15.0",
    "rollup": "^2.77.3"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "sideEffects": false
}
