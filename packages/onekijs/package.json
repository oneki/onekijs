{
  "name": "onekijs",
  "version": "0.11.0",
  "description": "Opiniated React framework built on top of Redux and React Router",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "exports": {
    ".": {
      "import": "./dist/index.modern.js",
      "require": "./dist/index.js"
    },
    "./next": {
      "import": "./next/dist/index.modern.js",
      "require": "./next/dist/index.js"
    },
    "./next-ssr": {
      "import": "./next-ssr/dist/index.modern.js",
      "require": "./next-ssr/dist/index.js"
    }
  },
  "module": "dist/index.module.js",
  "unpkg": "dist/index.umd.js",
  "scripts": {
    "build": "concurrently \"yarn build:onekijs\" \"yarn build:cra\" \"yarn build:next\" \"yarn build:next-ssr\" ",
    "build:onekijs": "microbundle -f es,cjs,modern --jsx React.createElement",
    "build:cra": "cd cra && microbundle -f es,cjs,modern --jsx React.createElement",
    "build:next": "cd next && microbundle -f es,cjs,modern --jsx React.createElement",
    "build:next-ssr": "cd next-ssr && microbundle -f es,cjs,modern --jsx React.createElement",
    "start": "microbundle watch -f es,cjs,modern --jsx React.createElement --max-old-space-size=8192"
  },
  "author": "Oneki",
  "license": "MIT",
  "dependencies": {
    "history": "^4.10.1",
    "immer": "^8.0.1",
    "query-string": "^6.11.1",
    "react-redux": "^7.2.0",
    "react-transition-group": "^4.4.1",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3",
    "reflect-metadata": "^0.1.13",
    "rfc4648": "^1.3.0",
    "text-encoding-shim": "^1.0.5",
    "@popperjs/core": "^2.4.4",
    "react-icons": "^4.2.0",
    "react-popper": "^2.2.3",
    "react-popper-tooltip": "^4.2.0",
    "react-virtual": "^2.6.2",
    "react-virtualized-auto-sizer": "^1.0.2",
    "react-window": "^1.8.5",
    "react-window-infinite-loader": "^1.0.5",
    "styled-components": "^5.1.1"    
  },
  "devDependencies": {
    "microbundle": "^0.13.3",
    "concurrently": "^6.2.0",
    "@types/react-transition-group": "^4.4.0",
    "@types/react-virtualized-auto-sizer": "^1.0.0",
    "@types/react-window": "^1.8.2"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16"
  },
  "sideEffects": false,
  "gitHead": "bd34719c6e0a4119cec72b64ce8fe6b35d1c342f"
}
