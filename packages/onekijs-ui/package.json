{
  "name": "onekijs-ui",
  "private": false,
  "version": "0.15.6",
  "description": "Opiniated React framework built on top of Next.js",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "exports": {
    ".": {
      "import": "./dist/index.module.js",
      "require": "./dist/index.js"
    }
  },
  "module": "dist/index.module.js",
  "unpkg": "dist/index.umd.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "rimraf dist && rollup --config rollup.config.js",
    "start": "rollup -w --config rollup.config.js"
  },
  "author": "Oneki",
  "license": "MIT",
  "dependencies": {
    "@clr/city": "^1.1.0",
    "@popperjs/core": "^2.4.4",
    "onekijs-framework": "^0.15.6",
    "react-icons": "^4.2.0",
    "react-popper": "^2.2.3",
    "react-popper-tooltip": "^4.2.0",
    "react-virtual": "^2.10.0",
    "react-virtualized-auto-sizer": "^1.0.2",
    "react-window": "^1.8.5",
    "react-window-infinite-loader": "^1.0.5",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@types/react-virtualized-auto-sizer": "^1.0.0",
    "@types/react-window": "^1.8.2",
    "microbundle": "^0.13.3",
    "rollup": "^2.77.3"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "sideEffects": false
}
