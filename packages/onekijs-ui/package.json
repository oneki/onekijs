{
    "name": "onekijs-ui",
    "version": "0.0.1",
    "description": "Widgets for Oneki.js library",
    "author": "oneki",
    "license": "MIT",
    "repository": "oneki/onekijs",
    "main": "dist/index.js",
    "module": "dist/esm/index.js",
    "engines": {
      "node": ">=8",
      "npm": ">=5"
    },
    "scripts": {
      "test": "cross-env CI=1 react-scripts test --env=jsdom",
      "test:watch": "react-scripts test --env=jsdom",
      "build": "babel --root-mode upward src -d dist/esm --source-maps",
      "start": "babel --root-mode upward src -d dist/esm --watch --source-maps",
      "prepare": "npm run build",
      "predeploy": "cd example && npm install && npm run build",
      "deploy": "gh-pages -d example/build"
    },
    "peerDependencies": {
      "react": "^15.0.0 || ^16.0.0",
      "react-dom": "^15.0.0 || ^16.0.0"
    },
    "dependencies": {
      "onekijs-core": "^0.7.1"
    },
    "sideEffects": false
  }
  