{
  "version": "0.21.2",
  "npmClient": "npm",
  "command": {
    "publish": {
      "ignoreChanges": [
        "ignored-file",
        "*.md"
      ],
      "conventionalCommits": true
    },
    "version": {
      "push": false,
      "message": "chore(release): publish %s",
      "allowBranch": "master"
    }
  },
  "packages": [
    "packages/*",
    "examples/*",
    "getting-started/*"
  ],
  "$schema": "node_modules/lerna/schemas/lerna-schema.json",
  "useNx": false
}
